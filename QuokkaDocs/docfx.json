{
  "metadata": [
    {
      "src": [
        {
          "src": "G:/Quokka/Quokka/",
          "files": [
            "Quokka.dll",
            "PlugBoard/Plugin_InstalledApps/Plugin/Plugin_InstalledApps.dll",
            "PlugBoard/Plugin_PortableApps/Plugin/Plugin_PortableApps.dll"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "../APIdocs/",
    "template": [
        "default",
        "modern",
        "templates/material"
    ],
    "globalMetadata": {
      "_appName": "QuokkaDocs",
      "_appTitle": "QuokkaDocs",
      "_appLogoPath": "../Resources/QuokkaLogo.svg",
      "_appFaviconPath": "../Resources/QuokkaLogo.svg",
      "_enableSearch": true,
      "_appFooter":"<footer></footer>",
      "_disableContribution":true,
      "pdf": false,
      "_disableNewTab": true
    },
    "postProcessors": ["ExtractSearchIndex"]
  }
}